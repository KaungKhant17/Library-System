<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="450.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.system.addmember.AddmemberController">
   <children>
      <HBox alignment="CENTER" prefHeight="450.0" prefWidth="750.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" spacing="15.0">
               <children>
                  <JFXTextField fx:id="idField" labelFloat="true" promptText="Member ID" />
                  <JFXTextField fx:id="nameField" labelFloat="true" promptText="Name" />
                  <JFXTextField fx:id="moblieField" labelFloat="true" promptText="Moblie" />
                  <JFXTextField fx:id="addressField" labelFloat="true" promptText="Address" />
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <JFXButton fx:id="saveBtn" onAction="#saveMemberInfo" prefHeight="40.0" prefWidth="120.0" text="Save" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
